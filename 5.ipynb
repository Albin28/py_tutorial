   "source": [
    "num = int(input(\"Enter a number: \"))\n",
    "sum_val = 0\n",
    "temp = num\n",
    "while temp > 0:\n",
    "    digit = temp % 10\n",
    "    sum_val += digit ** 3\n",
    "    temp //= 10\n",
    "\n",
    "if num == sum_val:\n",
    "    print(f\"{num} is an Armstrong number\")\n",
    "else:\n",
    "    print(f\"{num} is not an Armstrong number\")\n"
   ]
  }
