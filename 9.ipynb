   "source": [
    "binary = int(input(\"Enter a binary number: \"))\n",
    "decimal = 0\n",
    "for i, digit in enumerate(binary[::-1]):\n",
    "    decimal += int(digit) * (2 ** i)\n",
    "print(f\"Decimal equivalent: {decimal}\")\n"
   ]
  }
